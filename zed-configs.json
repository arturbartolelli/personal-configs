{
    "base_keymap": "VSCode",
    "vim_mode": true,
    "ui_font_size": 16,
    "buffer_font_size": 20,
    "theme": {
        "mode": "system",
        "light": "One Light",
        "dark": "Carbonfox"
    },
    "autosave": "off",
    "relative_line_numbers": true,
    "scrollbar": {
        "show": "never"
    },
    "use_autoclose": false,
    "cursor_blink": false
}
